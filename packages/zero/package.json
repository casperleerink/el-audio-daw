{
  "name": "@el-audio-daw/zero",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema.gen.ts",
    "./queries": "./src/queries.ts",
    "./mutators": "./src/mutators.ts"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "dev": "npx zero-cache-dev",
    "zero:generate": "drizzle-zero generate --schema '../db/src/schema.ts' --drizzle-kit-config '../db/drizzle.config.ts' --output './src/schema.gen.ts'"
  },
  "dependencies": {
    "@el-audio-daw/db": "workspace:*",
    "@rocicorp/zero": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@el-audio-daw/config": "workspace:*",
    "@types/node": "^22.12.0",
    "drizzle-zero": "^0.17.3",
    "typescript": "catalog:"
  }
}
